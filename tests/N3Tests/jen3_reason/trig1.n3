@prefix math: <http://www.w3.org/2000/10/swap/math#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix : <trig1.n3#> .
@base <http://example.org/> .

<#> rdfs:comment "Tests for trigonometry builtins" .

{ 90 math:radians ?r . ?r math:sin ?s . (?s 0) math:rounded ?ro } => { :test1 :has ?ro } .
{ ?r math:sin 1.0 . ?r math:degrees ?d . (?d 0) math:rounded ?ro } => { :test2 :has ?ro } .
{ 1.0 math:asin ?r . ?r math:degrees ?d . (?d 0) math:rounded ?ro } => { :test3 :has ?ro } .

{ 0 math:radians ?r . ?r math:cos ?c . (?c 0) math:rounded ?ro } => { :test4 :has ?ro } .
{ ?r math:cos 0.0 . ?r math:degrees ?d . (?d 0) math:rounded ?ro } => { :test5 :has ?ro } .
{ 0.0 math:acos ?r . ?r math:degrees ?d . (?d 0) math:rounded ?ro } => { :test6 :has ?ro } .

{ 45 math:radians ?r . ?r math:tan ?t . (?t 0) math:rounded ?ro } => { :test7 :has ?ro } .
{ ?r math:tan 1.0 . ?r math:degrees ?d . (?d 0) math:rounded ?ro } => { :test8 :has ?ro } .
{ 1.0 math:atan ?r . ?r math:degrees ?d . (?d 0) math:rounded ?ro } => { :test9 :has ?ro } .

{ 90 math:radians ?r . ?r math:sinh ?s . (?s 0) math:rounded ?ro } => { :test10 :has ?ro } .
{ ?r math:sinh 2.3013 . ?r math:degrees ?d . (?d 0) math:rounded ?ro } => { :test11 :has ?ro } .
{ 2.3013 math:asinh ?r . ?r math:degrees ?d . (?d 0) math:rounded ?ro } => { :test12 :has ?ro } .

{ 90 math:radians ?r . ?r math:cosh ?s . (?s 0) math:rounded ?ro } => { :test13 :has ?ro } .
{ ?r math:cosh 2.5092 . ?r math:degrees ?d . (?d 0) math:rounded ?ro } => { :test14 :has ?ro } .
{ 2.5092 math:acosh ?r . ?r math:degrees ?d . (?d 0) math:rounded ?ro } => { :test15 :has ?ro } .

{ 90 math:radians ?r . ?r math:tanh ?s . (?s 0) math:rounded ?ro } => { :test16 :has ?ro } .
{ ?r math:tanh 0.9172 . ?r math:degrees ?d . (?d 0) math:rounded ?ro } => { :test17 :has ?ro } .
{ 0.9172 math:atanh ?r . ?r math:degrees ?d . (?d 0) math:rounded ?ro } => { :test18 :has ?ro } .