@prefix math: <http://www.w3.org/2000/10/swap/math#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix fnon: <https://w3id.org/function/ontology/n3#> .

math:remainder a fno:Function ;
    vs:term_status  "testing" ;
    fnon:tldr "Calculates the object as the remainder of the division of the first integer by the second integer given in the subject list." ;
    dcterms:comment """`true` if and only if `$o` is the remainder of dividing `$s.1` by `$s.2`.""" ;
    fno:example ( [ a fno:Test ;
                dcterms:description """
Calculate the remainder of 10 divided by 3.
            """ ;
                rdfs:seeAlso <> ;
                fno:expression """
@prefix : <http://example.org/>.
@prefix math: <http://www.w3.org/2000/10/swap/math#> .

:Let :param (10 3) .

{
    :Let :param ?param .
    ?param math:remainder ?result .
}
=>
{
    :result :is ?result .
} .
            """ ;
                fno:result """
@prefix : <http://example.org/>.
:result :is 1.
            """ ] ) ;
    fno:name "remainder" ;
    fno:parameter ( 
        [ a fno:Parameter ;
            dcterms:description "" ;
            fno:mode "+" ;
            fno:predicate "$s" ;
            fno:type rdf:List ;
            fnon:position fnon:subject ;
            fnon:listElements ( [
                fno:predicate "$s.1" ;
                fno:mode "+" ;
                fno:type xsd:integer
            ] [
                fno:predicate "$s.2" ;
                fno:mode "+" ;
                fno:type xsd:integer
            ] )

        ] [ a fno:Parameter ;
            dcterms:description "" ;
            fno:mode "-" ;
            fno:predicate "$o" ;
            fnon:position fnon:object ;
            fno:type xsd:integer
        ] ) .
