@prefix crypto: <http://www.w3.org/2000/10/swap/crypto#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix fnon: <https://w3id.org/function/ontology/n3#> .

crypto:sha a fno:Function ;
    vs:term_status  "testing" ;
    fnon:tldr "Gets as object the SHA-1 hash of the subject." ;
    dcterms:comment "`true` if and only if `$o` is the SHA-1 hash of `$s`." ;
    fno:example ( [ a fno:Test ;
                dcterms:description """
Calculate the SHA-1 of the string "hello world".
            """ ;
                fno:expression """
@prefix : <http://example.org/> .
@prefix crypto: <http://www.w3.org/2000/10/swap/crypto#> .

{ "hello world" crypto:sha ?sha . } => { :result :is ?sha . } .
            """ ;
                fno:result """
@prefix : <http://example.org/>.
:result :is "2aae6c35c94fcfb415dbe95f408b9ce91ee846ed" .
            """ ]
     ) ;
    fno:name "SHA-1 hash" ;
    fno:parameter (
        [ a fno:Parameter ;
            dcterms:description "" ;
            fno:mode "+" ;
            fno:predicate "$s" ;
            fnon:position fnon:subject ;
        ] [ a fno:Parameter ;
            dcterms:description "" ;
            fno:predicate "$o" ;
            fno:mode "-" ;
            fnon:position fnon:object
        ] )  .
